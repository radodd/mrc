{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "jsx": "react-jsx"
  },
  "include": [
    "src/server.ts",
    "routes/**/*.ts",
    "src/**/*.ts",
    "src/controllers/resend.tsx"
  ]
}
